# Application URL (used for meta tags, SEO, and structured data)
VITE_APP_URL=https://your-domain.com

# Algolia Configuration
VITE_ALGOLIA_APP_ID=your-app-id
VITE_ALGOLIA_SEARCH_API_KEY=your-search-only-api-key
VITE_ALGOLIA_INDEX_NAME=bookmarks

# Docker Compose Configuration
NODE_ENV=production
APP_PORT=8080

# Note: FireBook uses Firebase Authentication and Firestore
# Firebase credentials are embedded in the client-side code
